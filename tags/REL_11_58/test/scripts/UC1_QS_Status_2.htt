INCLUDE scripts/ports

CLIENT 2
_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
__Host: localhost
__
_EXPECT . "root index"
_WAIT
END

CLIENT 1
_REQ localhost $QS_PORT_BASE
__GET /mod_qos_s.gif HTTP/1.1
__Host: localhost
__
_WAIT
END

CLIENT
_EXEC ./sleep.sh 55 1>/dev/null
_SLEEP 7100
_EXPECT EXEC "mod_qos\(200\): { \"scoreboard\": { \"open\": 896, \"waiting\": 128, \"read\": 0, \"write\": 0, .*{ \"max\": 256, \"busy\": 0 }, \"clientContentTypes\": { \"html\": 2,  \"css/js\": 0, \"images\": 1, \"other\": 0, \"304\": 0 } }"

_MATCH EXEC " ([0-9]+:[0-9]+:[0-9]+) " DATE
_EXEC grep "mod_qos(200)" logs/error_log | tail -1
_EXPECT EXEC "_1_"
_EXEC grep "mod_qos(200)" logs/error_log | grep -c $DATE  | awk '{print "_" $1 "_" }'

_REQ localhost $QS_PORT_BASE
__GET /status?auto HTTP/1.1
__Host: localhost
__
_EXPECT . "b;clientContentTypes\(html,css/js,images,other,304\): 2 0 1 0 0"
_WAIT


END

