# $Id: 006_QS_DenyRequestLine.htt,v 1.1 2012-03-14 19:51:39 pbuchbinder Exp $
#
#   <Location /htt/filter>
#     QS_DenyRequestLine +printable deny [\x00-\x1f\xf7-\xff]
#   </Location>

INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /htt/filter/index.html%0D%0Fmore HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "!==AS1=="
_EXPECT . "custom error page"
_EXPECT . "code=040"
_WAIT
_CLOSE
_SLEEP 200
_EXPECT EXEC "mod_qos\(040\): access denied, QS_DenyRequestLine rule id: printable"
_EXEC tail -1 logs/error_log

_REQ localhost $QS_PORT_BASE
__GET /htt/filter/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "==AS1=="
_WAIT
_CLOSE

END

SERVER $QS_PORT_BASE6

_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Type: text/plain
__Content-Length: AUTO
__
__==AS1==

END
