INCLUDE scripts/ports

CLIENT
_REQ localhost $QS_PORT_BASE
__GET /qos HTTP/1.1
__Host: localhost
__
_EXPECT . "><h2>mod_qos"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /qos?private HTTP/1.1
__Host: localhost
__
_EXPECT . "custom error"
_WAIT
_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /qos/merged?private HTTP/1.1
__Host: localhost
__
_EXPECT . "><h2>mod_qos"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /qos/merged?special HTTP/1.1
__Host: localhost
__
_EXPECT . "custom error"
_WAIT
_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /qos/merged/sub%03 HTTP/1.1
__Host: localhost
__
_EXPECT . "custom error"
_WAIT
_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /qos/off/sub%03 HTTP/1.1
__Host: localhost
__
_EXPECT . "max connections with keep-alive"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /qos/off/simple HTTP/1.1
__Host: localhost
__
_EXPECT . "custom error"
_WAIT

END
