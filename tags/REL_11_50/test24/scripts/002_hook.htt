INCLUDE scripts/ports

CLIENT

# Pre-Connection:
#    ...
#    10 mod_logio.c
#    10 mod_qos.c
#    10 mod_ssl.c
#    20 mod_qos.c
#    30 core.c

_REQ localhost $QS_PORT_BASE
__GET /info HTTP/1.1
__Host: localhost
__Connection: close
__
_EXPECT . "&nbsp;&nbsp; 10 <a href="#mod_logio.c">mod_logio.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_ssl.c\">mod_ssl.c</a> <br/>&nbsp;&nbsp; 20 <a href=\"#mod_qos.c\">mod_qos.c</a>"
_WAIT
_CLOSE


# Post-Read Request:
#    10 mod_unique_id.c
#    ...
#    10 mod_qos.c
#    10 mod_setenvif.c
#    10 mod_ssl.c
#    10 mod_setenvifplus.c
#    ....
#    10 mod_qos.c

_REQ localhost $QS_PORT_BASE
__GET /info HTTP/1.1
__Host: localhost
__Connection: close
__
_EXPECT . "&nbsp;&nbsp; 10 <a href=\"#mod_unique_id.c\">mod_unique_id.c</a> <br/>.*&nbsp;&nbsp; 10 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_setenvif.c\">mod_setenvif.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_ssl.c\">mod_ssl.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_setenvifplus.c\">mod_setenvifplus.c</a> <br/>.*&nbsp;&nbsp; 10 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>"
_WAIT
_CLOSE


# Header Parse:
#    00 mod_qos.c
#    10 mod_setenvif.c
#    10 mod_setenvifplus.c
#    00 mod_qos.c
#    10 mod_parp.c
#    10 mod_qos.c

_REQ localhost $QS_PORT_BASE
__GET /info HTTP/1.1
__Host: localhost
__Connection: close
__
_EXPECT . "&nbsp;&nbsp; 00 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_setenvif.c\">mod_setenvif.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_setenvifplus.c\">mod_setenvifplus.c</a> <br/>&nbsp;&nbsp; 00 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_parp.c\">mod_parp.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_qos.c\">mod_qos.c</a> <br/>"
_WAIT
_CLOSE

# Fixups:
#    ...
#    10 mod_setenvifplus.c
#    10 mod_qos.c
#    ...
_REQ localhost $QS_PORT_BASE
__GET /info HTTP/1.1
__Host: localhost
__Connection: close
__
_EXPECT . "&nbsp;&nbsp; 10 <a href=\"#mod_setenvifplus.c\">mod_setenvifplus.c</a> <br/>&nbsp;&nbsp; 10 <a href=\"#mod_qos.c\">mod_qos.c</a>"
_WAIT
_CLOSE

END
