INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /htt/index1.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "200"
_EXPECT . "mod_qos.052.: byte rate limit, rule: var={eventkbytespersec}.202."
_WAIT
_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /qos HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "200"
_EXPECT . "<!--3--><td>var=.eventkbytespersec.</a></td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td style=.background-color: rgb....,...,....;.>[0-9]{2,3}&nbsp;ms</td><td>202</td>"
_WAIT
_CLOSE

_LOOP 35
_REQ server1 $QS_PORT_BASE1
__GET /bbb/index.html HTTP/1.1
__Host: server1
__User-Agent: QS_EventKBytesPerSecLimit
__Connection: keep-alive
__
_EXPECT . "Revision"
_WAIT
_CLOSE
_SLEEP 2000
_END LOOP

_REQ localhost $QS_PORT_BASE
__GET /qos HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "200"
_EXPECT . "<!--3--><td>var=.eventkbytespersec.</a></td><td>-</td><td>-</td><td>-</td><td>-</td><td>-</td><td >0&nbsp;ms<"
_WAIT

END

SERVER $QS_PORT_BASE6
_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__
__==AS1==
_PIPE
_EXEC tail -1 ./logs/error_log

END

