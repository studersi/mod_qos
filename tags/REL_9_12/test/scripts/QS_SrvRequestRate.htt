INCLUDE scripts/ports

CLIENT
_REQ server1 SSL:$QS_PORT_BASE2
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: Mozilla/5.0
__
_EXPECT . "this is the root index"
_EXPECT . "200 OK"
_WAIT

_CLOSE

_REQ server1 SSL:$QS_PORT_BASE2
_SLEEP 12000
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: Mozilla/5.0
__
_EXPECT ERROR "Software caused connection abort"
_WAIT

END

CLIENT
_SLEEP 1000
_REQ localhost $QS_PORT_BASE
__GET /qos HTTP/1.1
__Host: localhost.qos.ch
__User-Agent: Mozilla/5.0
__
_EXPECT . "<!--0-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--1-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--2-->.*QS_SrvMaxConnPerIP..free ip entries.*895"
_EXPECT . "<!--3-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--4-->.*QS_SrvMaxConnPerIP..free ip entries.*895"
_WAIT
_CLOSE

_SLEEP 12000
_REQ localhost $QS_PORT_BASE
__GET /qos HTTP/1.1
__Host: localhost.qos.ch
__User-Agent: Mozilla/5.0
__
_EXPECT . "<!--0-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--1-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--2-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--3-->.*QS_SrvMaxConnPerIP..free ip entries.*896"
_EXPECT . "<!--4-->.*QS_SrvMaxConnPerIP..free ip entries.*895"
_WAIT
_CLOSE

_EXPECT EXEC "\[error\] mod_qos\(034\): access denied, QS_SrvMinDataRate rule \(in\): min=123, this connection=0"
_EXEC tail -1 logs/error_log

END
