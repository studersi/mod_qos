#
# QS_SrvMinDataRate  120 3000 10
#

INCLUDE scripts/ports

CLIENT

# slow response
_REQ localhost $QS_PORT_BASE
__GET /cgi/dvd.iso HTTP/1.1
__Host: localhost
__User-Agent: Mozilla/5.0 (X11; U; Linux i686; en-US; rv:1.8.1.5) Gecko/20070713 Firefox/2.0.0.5
__Connection: keep-alive
__
_SLEEP 11000
_RECV 100
_SLEEP 11000
_RECV 100
_EXPECT . "!END OF dvd.iso"
_WAIT
_CLOSE 
_EXPECT EXEC "mod_qos\(034\): access denied, QS_SrvMinDataRate rule \(out\): min=..., this connection=0, c=127.0.0.1"
_EXEC tail -1 logs/error_log

END

CLIENT 10

_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=10 HTTP/1.1
__Host: localhost
__
_WAIT
_CLOSE
END
