#
# $Id: 006_QS_SrvSerialize.htt,v 1.1 2015-06-10 15:57:48 pbuchbinder Exp $
#
#   QS_SrvSerialize            on
#   SetEnvIf                   User-Agent serializeme QS_SrvSerialize=1
#

INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 1
__Connection: keep-alive
__
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT
_SLEEP 100
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 2
__Connection: keep-alive
__
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT
_DATE:TIMER GET DSTART
_SLEEP 100
_REQ localhost $QS_PORT_BASE1
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__User-Agent: serializeme x
__Connection: keep-alive
__
_EXPECT . "done"
_WAIT
_CLOSE
_DATE:TIMER GET DEND
_OP $DEND SUB $DSTART DELTA
_IF "$DELTA" GT "1500"
_EXIT FAILED
_END IF
_SLEEP 100
_EXPECT EXEC  \"serializeme x\" 1 . \-"
_EXEC tail -1 logs/access1_log

END

CLIENT
_DATE:TIMER GET DSTART
_SLEEP 200
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 3
__Connection: keep-alive
__
_EXPECT . "done"
_WAIT
_CLOSE
_DATE:TIMER GET DEND
_OP $DEND SUB $DSTART DELTA
_IF "$DELTA" LT "3000"
_EXIT FAILED
_END IF
_SLEEP 100
_EXPECT EXEC  \"serializeme 3\" 3 \- s;"
_EXEC tail -1 logs/access_log
END
