INCLUDE scripts/ports

CLIENT
_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "test index.html"
_WAIT

_SLEEP 4000

_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "test index.html"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "Connection: close"
_EXPECT . "custom error page"
_WAIT

_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "!test index.html"
_WAIT

_CLOSE

_SLEEP 8000

_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "!test index.html"
_WAIT

_CLOSE

_SLEEP 4000

_REQ localhost $QS_PORT_BASE
__GET /test/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "test index.html"
_WAIT

END
