#
# $Id: 010_QS_KeepAliveTimeout-WORKER.htt,v 1.1 2015-08-27 19:26:09 pbuchbinder Exp $
#
#   KeepAliveTimeout         5
#   SetEnvIf                     User-Agent keepAliveTimeout10 QS_KeepAliveTimeout=10
#

INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: keepAliveTimeout10
__Connection: keep-alive
__
_EXPECT . "Keep-Alive: timeout=10, max=100"
_EXPECT . "this is the root index.html"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: keepAliveTimeout10
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT

_SLEEP 8000

_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: keepAliveTimeout10
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT

END
