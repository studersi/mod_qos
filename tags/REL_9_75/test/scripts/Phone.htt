INCLUDE scripts/ports

CLIENT 5

_REQ 127.0.0.1 $QS_PORT_BASE
__GET  /htt/index.html HTTP/1.1
__Host: 127.0.0.1
__Connection: keep-alive
__User-Agent: Phone
__
_WAIT
_CLOSE

END


SERVER $QS_PORT_BASE6 5
_RES
_WAIT
_SLEEP 1000
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/plain
__Connection: close
__
__==AS1==

_CLOSE

END
