
INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /htt/index1.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "!X-Login"
_EXPECT . "200"
_EXPECT . "AS1"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /htt/index1.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "!X-Login"
_EXPECT . "200"
_EXPECT . "AS2"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /htt/index1.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "!X-Login"
_EXPECT . "200"
_EXPECT . "AS3"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /htt/index1.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "code=067"
_WAIT
_CLOSE
_SLEEP 200

_EXPECT EXEC "mod_qos\(067\): access denied, QS_ClientEventLimitCount rule"
_EXEC tail -1 logs/error_log

END

SERVER $QS_PORT_BASE6
_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__X-Login: failed
__
__==AS1==

_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__X-Login: failed
__
__==AS2==

_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__X-Login: failed
__
__==AS3==

END
