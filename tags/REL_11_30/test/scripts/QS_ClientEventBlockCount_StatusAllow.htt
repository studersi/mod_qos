INCLUDE scripts/ports

CLIENT

_LOOP 10
_REQ localhost $QS_PORT_BASE
__GET /test/404/index.html HTTP/1.1
__Host: localhost
__User-Agent: curl
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "404 Not Found"
_WAIT
_END LOOP
_CLOSE

_LOOP 5
_REQ localhost $QS_PORT_BASE
__GET /test/404/index.html HTTP/1.1
__Host: localhost
__User-Agent: curl
__Connection: keep-alive
__Referer: /block
__
_EXPECT . "404 Not Found"
_WAIT
_CLOSE
_END LOOP

END
