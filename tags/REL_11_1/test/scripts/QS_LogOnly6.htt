#
# QS_EventRequestLimit
#

INCLUDE scripts/ports

CLIENT 4

_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: yoda
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT

END

CLIENT
_SLEEP 200
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: yoda
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE
_SLEEP 200
_EXPECT EXEC "mod_qos\(012\): access denied, QS_EventRequestLimit rule: var=\(YODA\)\(4\)"
_EXEC tail -1 logs/error_log

END
