INCLUDE scripts/ports

CLIENT
_REQ localhost $QS_PORT_BASE
__GET /aaa/index.html HTTP/1.1
__Host: localhost
__Req: 01
__
_EXPECT . "aaa index.html"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Req: 03
__
_EXPECT . "200 OK"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__Req: 04
__
_EXPECT . "200 OK"
_WAIT
_CLOSE

END
