# $Id: 101_QS_LocRequestPerSecLimitMatch.htt,v 1.1 2012-03-08 07:53:42 pbuchbinder Exp $
#
#   QS_LocRequestPerSecLimitMatch ^/limitrs/       10
#


INCLUDE scripts/ports

# about 80 req/sec (without mod_qos
CLIENT 4
_AUTO_CLOSE on
_RPS 20 120
_REQ localhost $QS_PORT_BASE
__GET /limitrs/index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_WAIT
_END RPS

END

CLIENT
_SLEEP 120000
_EXPECT EXEC "r/s;1[0-9]?;"
_EXEC tail -1 logs/qs_log

_EXPECT EXEC "mod_qos\(050\): request rate limit, rule: \^/limitrs/\.\*\(10\)"
_EXEC tail -1 logs/error_log
END
