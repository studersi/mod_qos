INCLUDE scripts/ports

CLIENT 4

_REQ localhost SSL:$QS_PORT_BASE2
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: EL5
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT 1
_SLEEP 200
_REQ localhost SSL:$QS_PORT_BASE2
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: EL5
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT 1
_SLEEP 400
_REQ localhost SSL:$QS_PORT_BASE2
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: EL6
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT 1
_SLEEP 400
_REQ localhost SSL:$QS_PORT_BASE2
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: XXX
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE

END

CLIENT 1
_SLEEP 400
_REQ localhost SSL:$QS_PORT_BASE2
__GET /cgi/sleep.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__User-Agent: EL5
__
_EXPECT . "!200 OK"
_EXPECT . "!done"
_EXPECT . "500 Internal Server Error"
_WAIT
_CLOSE
_SLEEP 100
_EXPECT EXEC "mod_qos\(012\): access denied, QS_EventRequestLimit rule: var=\(EL=1\)\(5\), concurrent requests=6"
_EXEC tail -1 logs/error_log

END
