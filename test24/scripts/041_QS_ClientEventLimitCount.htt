#
# $Id$
#
# QS_ClientEventLimitCount  3 10
# QS_SetEnvIfStatus         414 QS_Limit
#

INCLUDE scripts/ports

EXEC ./ctl.sh restart -D real_ip 2>/dev/null 1>/dev/null


CLIENT

_REQ [::1] $QS_PORT_BASE10
__GET /index.html HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 1
__Host: ip6-localhost
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT
_CLOSE

_LOOP 3
_REQ [::1] $QS_PORT_BASE10
__GET  HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 1
__Host: ip6-localhost
__Connection: keep-alive
__
_EXPECT . "414 Request-URI Too Long"
_WAIT
_CLOSE
_END LOOP

_REQ [::1] $QS_PORT_BASE10
__GET /index.html HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 1
__Host: ip6-localhost
__Connection: keep-alive
__
_EXPECT . "!this is the root index.html"
_EXPECT . "503 Service Unavailable"
_WAIT
_CLOSE
_SLEEP 100
_EXPECT EXEC "mod_qos\(067\): access denied, QS_ClientEventLimitCount rule: event=QS_Limit, max=3, current=3, age=0, c=::1"
_EXEC tail -1 logs/error_log

_SLEEP 11000

_REQ [::1] $QS_PORT_BASE10
__GET /index.html HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 1
__Host: ip6-localhost
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT
_CLOSE

END

CLIENT

_REQ server1 $QS_PORT_BASE
__GET /index.html HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 2
__Host: server1
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT
_CLOSE

_LOOP 7
_SLEEP 1000

_REQ server1 $QS_PORT_BASE
__GET /index.html HTTP/1.1
__User-Agent: 041_QS_ClientEventLimitCount.htt 2
__Host: server1
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT
_CLOSE
_END LOOP

END

BLOCK FINALLY
_EXEC ./ctl.sh restart 2>/dev/null 1>/dev/null
END
