#
# $Id$
#   SetEnvIf                     User-Agent delay2100 QS_Delay=2100
#

INCLUDE scripts/ports

CLIENT
_DATE:TIMER GET DSTART
_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
__Host: localhost
__User-Agent: delay2100
__Connection: keep-alive
__
_EXPECT . "this is the root index.html"
_WAIT
_CLOSE
_DATE:TIMER GET DEND
_OP $DEND SUB $DSTART DELTA
_IF "$DELTA" LT "2100"
_EXIT FAILED
_END IF
_IF "$DELTA" GT "2500"
_EXIT FAILED
_END IF
_SLEEP 100
_EXPECT EXEC "\"GET /index.html HTTP/1.1\" 200 159 \"delay2100" 2 1 L; 1 id"
_EXEC tail -1 logs/access_log
END
