INCLUDE scripts/ports

CLIENT
_SET ID=10000000000
_MATH:RAND 1000000 9000000 RND

_EXEC ./ctl.sh stop
_SLEEP 1000
_EXEC ../httpd/httpd -d `pwd` -f conf/ucn.conf -D ucnu

_EXEC echo "A" >> logs/qs_log
# pre
_RPS 1000 140
_LOOP 10
_SET STDID=${RND}AAABBCCDDEEFFGGHH${ID}
_REQ localhost $QS_PORT_BASE $counter
__GET /index.html?A${STDID} HTTP/1.1
__Host: localhost
__User-Agent: htt
__
_WAIT
_END LOOP
_CLOSE
_END RPS

_EXEC echo "B" >> logs/qs_log
# inc
_LOOP 1000
_MATH:RAND 1000000 9000000 RND
_LOOP 10
_SET STDID=${RND}AAABBCCDDEEFFGGHH${ID}
_REQ localhost $QS_PORT_BASE $counter
__GET /index.html?B${STDID} HTTP/1.1
__Host: localhost
__User-Agent: htt
__
_WAIT
_MATH:OP $ID ADD 1 ID
_END LOOP
_CLOSE
_END LOOP

_EXEC echo "C" >> logs/qs_log
# post
_RPS 1000 140
_LOOP 10
_REQ localhost $QS_PORT_BASE $counter
_SET STDID=${RND}AAABBCCDDEEFFGGHH${ID}
__GET /index.html?C${STDID} HTTP/1.1
__Host: localhost
__User-Agent: htt
__
_WAIT
_END LOOP
_CLOSE
_END RPS

_EXEC echo "E" >> logs/qs_log

END

CLIENT
_LOOP 5
_SLEEP 60000
_EXEC top -b -n 1 | grep qslog >> logs/qs_log
_END LOOP
