
INCLUDE scripts/ports

CLIENT 10

_LOOP 4
_REQ localhost $QS_PORT_BASE
__GET /a/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__
_EXPECT . "done"
_EXPECT . "200 OK"
_WAIT
_CLOSE
_END LOOP

END

CLIENT 10

_AUTO_CLOSE on
_LOOP 120
_LOOP 5
_REQ localhost $QS_PORT_BASE
__GET /status?auto HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "BusyWorkers"
_EXPECT . "200 OK"
_WAIT
_END LOOP
_END LOOP

END

