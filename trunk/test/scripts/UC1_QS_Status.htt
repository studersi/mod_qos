INCLUDE scripts/ports

CLIENT 2
_EXEC ./sleep.sh 55 1>/dev/null
_REQ localhost $QS_PORT_BASE
__GET /index.html HTTP/1.1
_SLEEP 7000
__Host: localhost
__
_EXPECT . "root index"
_WAIT
END

CLIENT
_EXEC ./sleep.sh 55 1>/dev/null
_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=7 HTTP/1.1
__Host: localhost
__
_EXPECT . "done"
_WAIT
END

CLIENT
_EXEC ./sleep.sh 55 1>/dev/null
_SLEEP 7100
_EXPECT EXEC "mod_qos\(200\): { \"scoreboard\": { \"open\": 896, \"waiting\": 125, \"read\": 2, \"write\": 1, .*{ \"max\": 256, \"busy\": 3 }"

_MATCH EXEC " ([0-9]+:[0-9]+:[0-9]+) " DATE
_EXEC grep "mod_qos(200)" logs/error_log | tail -1
_EXPECT EXEC "_1_"
_EXEC grep "mod_qos(200)" logs/error_log | grep -c $DATE  | awk '{print "_" $1 "_" }'

END

