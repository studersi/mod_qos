INCLUDE scripts/ports

CLIENT 2
_REQ localhost $QS_PORT_BASE
__GET /htt/a/sleep/1 HTTP/1.1
__Host: localhost
__
_EXPECT . "AS1"
_WAIT
_CLOSE

_SLEEP 100

_REQ localhost $QS_PORT_BASE
__GET /htt/a/sleep/2 HTTP/1.1
__Host: localhost
__
_EXPECT . "AS2"
_WAIT

END

CLIENT
_SLEEP 500
_REQ localhost $QS_PORT_BASE
__GET /htt/a/sleep/3 HTTP/1.1
__Host: localhost
__
_EXPECT . "500 Internal Server Error"
_WAIT
_CLOSE

END

SERVER $QS_PORT_BASE6 2
_RES
_SLEEP 1000
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/text
__
__==AS1==
_CLOSE

_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/text
__
__==AS2==
_CLOSE

END
