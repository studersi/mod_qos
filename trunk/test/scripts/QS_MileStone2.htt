INCLUDE scripts/ports

CLIENT
_REQ localhost $QS_PORT_BASE
__GET /htt/any.gif HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Cookie: QSSCD=$QSSCD
__
_EXPECT . "200"
_EXPECT . "!Set-Cookie"
_WAIT

_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /htt/logo.gif HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Cookie: QSSCD=$QSSCD
__
_EXPECT . "HTTP/1.. 500"
_EXPECT . "code=047"
_EXPECT . "!Set-Cookie"
_WAIT

_CLOSE

_REQ localhost $QS_PORT_BASE
__GET /htt/any.gif HTTP/1.1
__Host: localhost
__Connection: keep-alive
__Cookie: QSSCD=$QSSCD
__
_EXPECT . "200"
_EXPECT . "!Set-Cookie"
_WAIT

END

SERVER $QS_PORT_BASE6
_RES
_EXPECT . "!Cookie"
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__
__==AS1==

_CLOSE

_RES
_EXPECT . "!Cookie"
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__
__==AS1==

END
