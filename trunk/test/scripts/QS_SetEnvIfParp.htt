INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__POST /error-docs/error.shtml HTTP/1.1
__Host: localhost
__Content-Length: AUTO
__Content-Type: application/x-www-form-urlencoded
__
__postdata=match&body=more
_EXPECT . "200"
_EXPECT . "_4parp.more."
_EXPECT . "_5parp.match."
_WAIT

_SLEEP 7000
_CLOSE

_REQ localhost $QS_PORT_BASE
__POST /error-docs/error.shtml HTTP/1.1
__Host: localhost
__Content-Length: AUTO
__Content-Type: application/x-www-form-urlencoded
__
__postdata=match&body=more
_EXPECT . "200"
_EXPECT . "_4parp.more."
_EXPECT . "_5parp.match."
_WAIT

END

CLIENT 3
_SLEEP 1000

_REQ localhost $QS_PORT_BASE
__POST /cgi/sleep.cgi?s=4 HTTP/1.1
__Host: localhost
__Content-Length: AUTO
__Content-Type: application/x-www-form-urlencoded
__
__postdata=match&body=more
_EXPECT . "200"
_EXPECT . "done"
_WAIT

END

CLIENT
_SLEEP 3000

_REQ localhost $QS_PORT_BASE
__POST /cgi/sleep.cgi?s=4 HTTP/1.1
__Host: localhost
__Content-Length: AUTO
__Content-Type: application/x-www-form-urlencoded
__
__postdata=match&body=more
_EXPECT . "500 Internal Server Error"
_EXPECT . "!done"
_EXPECT . "code=012"
_WAIT

END
