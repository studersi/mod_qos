INCLUDE scripts/ports
INCLUDE scripts/maxclients

CLIENT $maxclients

_REQ server1 $QS_PORT_BASE1
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_WAIT

END

CLIENT
_SLEEP 200
_LOOP 300
_REQ server1 $QS_PORT_BASE1
__GET /index.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_WAIT
_CLOSE
_END LOOP
END
