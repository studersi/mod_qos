INCLUDE scripts/ports

# consume all connections ...
CLIENT 5
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=3 HTTP/1.1
__Host: localhost
__
_EXPECT . "200 OK"
