# 
# MaxRequestWorkers      50
# QS_ClientPrefer        20
#


INCLUDE scripts/ports

EXEC ./ctl.sh restart -D QS_ClientPrefer
SLEEP 1000

CLIENT 10

_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_CLOSE

_LOOP 20
_LOOP 10
_REQ localhost $QS_PORT_BASE
__GET /errorpages/cookie.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "Cookie Check Page"
_WAIT

_REQ localhost $QS_PORT_BASE
__GET /errorpages/cookie.html HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "Cookie Check Page"
_WAIT
_CLOSE
_END LOOP
_END LOOP

END

CLIENT
_SLEEP 400
_REQ localhost $QS_PORT_BASE
__GET /qos?auto HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "QS_AllConn: 11"
_EXPECT . "QS_ClientPrefer;10\[\]: 11"
_WAIT
_CLOSE

_SLEEP 2500

_REQ localhost $QS_PORT_BASE
__GET /qos?auto HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "QS_AllConn: 1"
_EXPECT . "QS_ClientPrefer;10\[\]: 1"
_WAIT
_CLOSE

END

BLOCK FINALLY
_EXEC ./ctl.sh stop 2>/dev/null 1>/dev/null
_SLEEP 1000
_EXEC ./ctl.sh start 2>/dev/null 1>/dev/null
END
