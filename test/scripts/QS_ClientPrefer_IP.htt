INCLUDE scripts/ports

CLIENT 50
_REQ server1 $QS_PORT_BASE1
__GET /login/vip_ip.cgi HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT

_CLOSE

_SLEEP 1000
_ERROR .*
_REQ server1 $QS_PORT_BASE1
__GET /cgi/sleep.cgi?s=5 HTTP/1.1
__Host: localhost
__Connection: keep-alive
__
_WAIT
_END ERROR
END

