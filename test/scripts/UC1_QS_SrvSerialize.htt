INCLUDE scripts/ports

CLIENT

_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=1&id=1 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 1
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT

END

CLIENT
_SLEEP 100
_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=1&id=2 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 2
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT

END

CLIENT

_DATE:TIMER GET DSTART
_SLEEP 200
_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=1&id=3 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 3
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_DATE:TIMER GET DEND
_OP $DEND SUB $DSTART DELTA
_IF "$DELTA" LT "3000"
_EXIT FAILED
_END IF

END

CLIENT

_DATE:TIMER GET DSTART
_SLEEP 250
_REQ localhost $QS_PORT_BASE
__GET /cgi-local/sleep.cgi?s=1&id=4 HTTP/1.1
__Host: localhost
__User-Agent: serializeme 4
__
_EXPECT . "200 OK"
_EXPECT . "done"
_WAIT
_DATE:TIMER GET DEND
_OP $DEND SUB $DSTART DELTA
_IF "$DELTA" LT "4000"
_EXIT FAILED
_END IF

END
