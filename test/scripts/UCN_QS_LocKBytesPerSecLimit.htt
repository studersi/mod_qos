#   QS_LocRequestLimitDefault                              10
#   QS_LocRequestLimitMatch       "^.*\.gif$"              5
#   QS_LocRequestLimit            /images                  8
#   QS_LocRequestLimit            /a                       4
#   QS_LocKBytesPerSecLimit       /a                       3000
#   QS_LocRequestLimitMatch       "^(/b/|/c/).*$"          3
