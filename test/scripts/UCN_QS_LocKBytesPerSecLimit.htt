INCLUDE scripts/ports

CLIENT
_EXEC ./sleep.sh

# ~5000 kbytes/sec without limitation
_LOOP 25
_RPS 10 5
_REQ localhost $QS_PORT_BASE
__GET /a/image.iso HTTP/1.1
__Host: localhost
__
_EXPECT . "200 OK"
_WAIT
_END RPS
_CLOSE
_END LOOP

_EXPECT EXEC ";b/s;[23][0-9]{6,6};"
_EXEC tail -1 logs/qs_log

END
