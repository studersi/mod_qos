#
# test QS_LocRequestLimit and QS_ErrorPage directive
#
#   QS_LocRequestLimit         /cgi            5
#
# $Id: QS_LocRequestLimit_7.htt,v 1.1 2012-01-23 06:53:58 pbuchbinder Exp $
#


INCLUDE scripts/ports

CLIENT 5
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
_FLUSH
_SLEEP 300
__
_EXPECT . "200 OK"
_EXPECT . "done"
_EXPECT . "!code=010"
_EXPECT . "!500 Internal Server Error"
_WAIT
_CLOSE

END

CLIENT 5
_REQ localhost $QS_PORT_BASE
__GET /cgi/sleep.cgi?s=1 HTTP/1.1
__Host: localhost
_FLUSH
_SLEEP 700
__
_EXPECT . "!200 OK"
_EXPECT . "!done"
_EXPECT . "code=010"
_EXPECT . "500 Internal Server Error"
_WAIT
_CLOSE

END
