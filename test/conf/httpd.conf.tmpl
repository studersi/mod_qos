PidFile                 ##ROOT##/logs/apache.pid
CoreDumpDirectory       ##ROOT##/logs/

#QS_Chroot               ##ROOT##
#LoadFile                /lib/libgcc_s.so.1

TypesConfig             conf/mime.types
